# Set javac settings from JDT prefs
-include: ${workspace}/cnf/includes/jdt.bnd, ${workspace}/cnf/includes/bndtools.bnd

Test-Cases: ${classes;CONCRETE;PUBLIC;NAMED;*Test*} 

# These are bundles that are not required for building but for debugging when doing
# code inspection.
-buildpath.debug: \
	org.eclipse.osgi,\
	org.eclipse.core.resources,\
	org.eclipse.e4.ui.workbench.swt

-conditionalpackage: \
	aQute.lib.*;-split-package:=merge-first, \
	aQute.libg.*;-split-package:=merge-first

-buildpath: \
	aQute.libg;version=snapshot,\
	bndtools.core;version=snapshot,\
	bndtools.builder;version=snapshot,\
	org.junit.platform.commons;version=latest,\
	org.junit.platform.engine;version=latest,\
	assertj-core;version=latest,\
	org.opentest4j;version=latest,\
	org.apiguardian:apiguardian-api;version=latest,\
	org.junit.jupiter.api;version=latest,\
	org.junit.jupiter.engine;version=latest,\
	org.junit.jupiter.params;version=latest,\
	org.eclipse.jdt.core;version=3.16.0,\
	org.eclipse.jdt.ui;version=3.16.0,\
	org.eclipse.core.runtime;version=3.15.100,\
	org.eclipse.equinox.common;version=3.10.200,\
	org.eclipse.equinox.registry,\
	org.osgi.service.component.annotations;version=1.3.0,\
	org.eclipse.osgi;version=3.13.200,\
	org.eclipse.equinox.app,\
	org.eclipse.ui.ide.application,\
	org.eclipse.core.resources;version=3.13.200,\
	org.eclipse.jface.text;version=3.15.0,\
	org.eclipse.jdt.core.manipulation;version=1.11.0,\
	org.eclipse.ui.workbench;version=3.112.100,\
	org.eclipse.core.jobs;version=3.10.200,\
	org.eclipse.ui.ide;version=3.14.200,\
	biz.aQute.bndlib;version=5.1.0,\
	org.eclipse.swt.win32.win32.x86_64;version=latest,\
	org.osgi.service.component.annotations;version=1.4.0,\
	org.eclipse.jdt.launching;version=3.12.0

Import-Package: org.eclipse.ui.dialogs;bundle-symbolic-name="org.eclipse.ui.workbench";ui.workbench="split",\
	org.eclipse.ui;bundle-symbolic-name="org.eclipse.ui.workbench";ui.workbench="split",\
	${eclipse.importpackage},*

-includeresource: {plugin.xml=_plugin.xml}
Private-Package: bndtools.core.test*
